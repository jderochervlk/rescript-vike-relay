{
  "type": "module",
  "scripts": {
    "dev": "npm run server:dev",
    "prod": "npm run build && npm run server:prod",
    "build": "npm run relay:build && npm run res:build && vite build",
    "server:dev": "node ./server",
    "server:prod": "cross-env NODE_ENV=production node ./server",
    "res:build": "rescript build -with-deps",
    "res:dev": "rescript build -w -with-deps",
    "relay:build": "rescript-relay-compiler",
    "relay:dev": "rescript-relay-compiler --watch",
    "postinstall": "patch-package",
    "start": "concurrently --prefix -c red,blue,green --names ReScript,Relay,Vite \"npm run res:dev\" \"npm run relay:dev\" \"npm run dev\""
  },
  "dependencies": {
    "@fattafatta/rescript-jotai": "^0.3.0",
    "@rescript/core": "^0.5.0",
    "@rescript/react": "^0.11.0",
    "@vitejs/plugin-react": "^4.0.4",
    "browser-monads": "^1.0.0",
    "classnames": "^2.3.2",
    "compression": "^1.7.4",
    "cross-env": "^7.0.3",
    "express": "^4.18.2",
    "jotai": "^2.4.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-loading-skeleton": "^3.3.1",
    "react-relay": "^15",
    "react-relay-network-modern": "^6.2.2",
    "react-relay-network-modern-ssr": "file:.yalc/react-relay-network-modern-ssr",
    "react-ssr-prepass": "^1.5.0",
    "relay-runtime": "^15.0.0",
    "rescript": "^11.0.0-rc.4",
    "rescript-relay": "^2",
    "rescript-struct": "^5.0.1",
    "rescript-webapi": "^0.9.0",
    "serialize-javascript": "^6.0.1",
    "sirv": "^2.0.3",
    "vike": "^0.4.142",
    "vite": "^4.4.9"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.16",
    "concurrently": "^8.2.1",
    "gentype": "^4.5.0",
    "graphql": "^16.8.1",
    "patch-package": "^8.0.0",
    "postcss": "^8.4.31",
    "relay-compiler": "^15.0.0",
    "tailwindcss": "^3.3.3"
  }
}